{
  "resource_limits": {
    "user_space_size": "300MB"
  },
  "process": {
    "default_stack_size": "4MB",
    "default_heap_size": "32MB",
    "default_mmap_size": "100MB"
  },
  "entry_points": [
    "/bin"
  ],
  "env": {
    "default": [
      "OCCLUM=yes"
    ],
    "untrusted": [
      "EXAMPLE"
    ]
  },
  "mount": [
    {
      "options": {
        "layers": [
          {
            "options": {
              "MAC": "0c-fc-fa-28-15-a2-1b-17-ca-42-1a-69-fb-68-58-2e"
            },
            "source": "./build/initfs/__ROOT",
            "target": "/",
            "type": "sefs"
          },
          {
            "source": "./run/initfs/__ROOT",
            "target": "/",
            "type": "sefs"
          }
        ]
      },
      "target": "/",
      "type": "unionfs"
    },
    {
      "target": "/proc",
      "type": "procfs"
    },
    {
      "target": "/dev",
      "type": "devfs"
    }
  ]
}